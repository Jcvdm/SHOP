{
  "name": "shop",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:mem": "node --max-old-space-size=6144 node_modules/vite/bin/vite.js dev",
    "dev:development": "vite dev --mode development",
    "build": "vite build",
    "build:development": "vite build --mode development",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "generate:types": "powershell -ExecutionPolicy Bypass -File scripts/generate-types.ps1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.22.0",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.544.0",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-vercel": "^5.6.3",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^22",
    "@vitest/browser": "^3.2.3",
    "clsx": "^2.1.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "playwright": "^1.53.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "bigger-picture": "^1.1.19",
    "bits-ui": "^2.14.4",
    "browser-image-compression": "^2.0.2",
    "filepond": "^4.32.9",
    "filepond-plugin-file-validate-type": "^1.2.9",
    "filepond-plugin-image-preview": "^4.6.12",
    "jszip": "^3.10.1",
    "lucide-svelte": "^0.544.0",
    "puppeteer": "^24.24.0",
    "svelte-filepond": "^0.2.2",
    "undici": "^7.16.0"
  }
}